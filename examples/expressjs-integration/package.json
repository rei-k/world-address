{
  "name": "expressjs-address-integration",
  "version": "1.0.0",
  "description": "Express.js integration example with @vey/core SDK including webhooks and authentication",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "express",
    "address",
    "validation",
    "vey",
    "webhooks",
    "authentication"
  ],
  "author": "Vey Team",
  "license": "MIT",
  "dependencies": {
    "@vey/core": "file:../../sdk/core",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "express-validator": "^7.0.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "prettier": "^3.0.0",
    "eslint": "^8.0.0"
  }
}
