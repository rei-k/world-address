{
  "name": "@vey/veyexpress",
  "version": "1.0.0",
  "description": "VeyExpress - Comprehensive Logistics Integration Platform with 254-country support, multi-carrier integration, and AI-powered optimization",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "logistics",
    "delivery",
    "shipping",
    "multi-carrier",
    "address-validation",
    "tracking",
    "waybill",
    "cross-border",
    "ai-prediction",
    "route-optimization",
    "254-countries"
  ],
  "author": "Vey Team",
  "license": "MIT",
  "dependencies": {
    "@vey/core": "workspace:*",
    "@vey/carriers": "workspace:*",
    "express": "^4.18.0",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.0",
    "typescript": "^5.0.0",
    "ts-node-dev": "^2.0.0",
    "eslint": "^8.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
